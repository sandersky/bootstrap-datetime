{
    "env": {
        "amd": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "brace-style": [2, "1tbs"],
        "block-scoped-var": 2,
        "camelcase": 2,
        "complexity": [2, 7],
        "consistent-return": 2,
        "consistent-this": [2, "self"],
        "curly": [2, "all"],
        "default-case": 2,
        "dot-notation": 2,
        "eol-last": 2,
        "eqeqeq": [2, "allow-null"],
        "func-names": 0,
        "func-style": [2, "declaration"],
        "guard-for-in": 2,
        "max-depth": 0,
        "max-len": 0,
        "max-nested-callbacks": [2, 3],
        "max-params": 0,
        "max-statements": 0,
        "new-cap": 2,
        "new-parens": 2,
        "no-alert": 2,
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-catch-shadow": 2,
        "no-comma-dangle": 0,
        "no-delete-var": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-label": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-strict": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-global-strict": 2,
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-loop-func": 0,
        "no-mixed-spaces-and-tabs": [2, false],
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-nested-ternary": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-plusplus": 0,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-space-before-semi": 2,
        "no-spaced-func": 2,
        "no-ternary": 0,
        "no-trailing-spaces": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-undefined": 2,
        "no-underscore-dangle": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "no-warning-comments": 1,
        "no-with": 2,
        "no-wrap-func": 2,
        "one-var": 0,
        "quote-props": 0,
        "quotes": [2, "single"],
        "radix": 0,
        "semi": [2, "always"],
        "sort-vars": 2,
        "space-after-keywords": [2, "always"],
        "space-in-brackets": [2, "never"],
        "space-infix-ops": 2,
        "space-return-throw-case": 2,
        "space-unary-word-ops": 2,
        "strict": 2,
        "valid-jsdoc": [2, {
            "prefers": {
                "requireParamDescription": true,
                "requireReturn": false,
                "return": "returns"
            }
        }],
        "valid-typeof": 2,
        "wrap-iife": 2,
        "wrap-regex": 0,
        "yoda": [2, "never"]
    }
}
